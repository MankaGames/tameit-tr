# game/scenes/chapter05/coldspot.rpy:70
translate turkish scene_coldspot_ch05_46a7ccf3:

    # lee_think "{e=smiley}{e=star_struck}{e=smiling_face_with_hearts}{e=smiling_face}"
    lee_think "Vay! Onu evcillestirmeyi basardim! Ve sadece bu da degil."
    lee_think "Cildi yumusacaik ve çok güzel! Ayrica saçlari da kürk gibi hissettiriyor."
    lee_think "Ayni -gerçek bir kedi gibi- uyurken kulaklari biraz segiriyor."
    lee_think "Ayrica çok hassas, onu islatana kadar amcigina zar zor dokunabildim."
    lee_think "Kimse ona böyle bir sey yapmadi, bence. Daha önce kimse ona dokunmamistir."

# game/scenes/chapter05/coldspot.rpy:74
translate turkish scene_coldspot_ch05_0218fdc9:

    # lee_think "{e=catgirl}{e=sleep}{e=no}{e=t-shirt}{e=heart_eyes}"
    lee_think "Yataga gitmeden önce tisörtünü çikardi"
    lee_think "sanki ona tekrar dokunacagimi biliyomus gibi."
    lee_think "Bana bile her gece uyuyor numarasi yapiyormus gibi geliyor,"
    lee_think "ama aslinda sadece gelip onunla oynamami bekliyor."

# game/scenes/chapter05/coldspot.rpy:76
translate turkish scene_coldspot_ch05_79127666:

    # lee_think "{e=smiling_face_with_hearts}{e=smiling_face}"
