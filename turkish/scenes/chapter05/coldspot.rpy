# game/scenes/chapter05/coldspot.rpy:70
translate turkish scene_coldspot_ch05_46a7ccf3:

    # lee_think "{e=smiley}{e=star_struck}{e=smiling_face_with_hearts}{e=smiling_face}"
    lee_think "Vay! Onu evcillestirmeyi basardım! Ve sadece bu da degil."
    lee_think "Cildi yumusacaık ve çok güzel! Ayrıca saçları da kürk gibi hissettiriyor."
    lee_think "Aynı -gerçek bir kedi gibi- uyurken kulakları biraz segiriyor."
    lee_think "Ayrıca çok hassas, onu ıslatana kadar amcıgına zar zor dokunabildim."
    lee_think "Kimse ona böyle bir sey yapmadı, bence. Daha önce kimse ona dokunmamıstır."

# game/scenes/chapter05/coldspot.rpy:74
translate turkish scene_coldspot_ch05_0218fdc9:

    # lee_think "{e=catgirl}{e=sleep}{e=no}{e=t-shirt}{e=heart_eyes}"
    lee_think "Yataga gitmeden önce tisörtünü çıkardı"
    lee_think "sanki ona tekrar dokunacagımı biliyomus gibi."
    lee_think "Bana bile her gece uyuyor numarası yapıyormus gibi geliyor,"
    lee_think "ama aslında sadece gelip onunla oynamamı bekliyor."

