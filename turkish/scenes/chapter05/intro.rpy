translate turkish scene_chapter05_intro_43576a43:

    # lee_think "{e=dash}{e=lightning}{e=confounded face}{e=weary_face}"
    lee_think "Hava bok gibi. Ayrica rüzgar nerdeyse beni uçuracak!"

# game/scenes/chapter05/intro.rpy:26
translate turkish scene_chapter05_intro_905a5088:

    # lee_think "{e=lee}{e=arrow_right}{e=bungalow4}{e=frowning_face}"
    lee_think "Evde kalmaktan baska carem yok."
    lee_think "Iyi ki disarisi çilgina dönmeden bungalovu bitirmeyi basardim."

# game/scenes/chapter05/intro.rpy:35
translate turkish scene_chapter05_intro_09ae621c:

    # lee_think "{e=hushed_face}{e=rain}{e=no}{e=bonfire}"
    lee_think "Lanet olsun ates tamemen söndü.."
    lee_think "Bu havaya bakinca — pek de sasirdigimi söyleyemem."

# game/scenes/chapter05/intro.rpy:36
translate turkish scene_chapter05_intro_4da11327:

    # lee_think "{e=thinking_face}{e=light_bulb}{e=fireplace}{e=arrow_right}{e=bungalow4}"
    lee_think "Simdi ne yapacagim? Ne yemek yapabiliyorum ne de isinabiliyorum."
    lee_think "Iceride ates yakmanin bir\u00A0yolunu bulmaliyim."

# game/scenes/chapter05/intro.rpy:38
translate turkish scene_chapter05_intro_3aa89023:

    # lee "{e=fearful_face}{e=scream}{e=storm}{e=exclamation}"
    lee "Bu artik firtina degil — kasirgaya döndü aq ya da hortum mu desem?"
    lee "Neyse ne ya. Sonuçta her seyin icine etti!"

# game/scenes/chapter05/intro.rpy:58
translate turkish scene_chapter05_intro_529c7150:

    # lee_think "{e=arrow_right}{e=plane_door}{e=lock}"
    lee_think "Rüzgarin uçurmasini istemiyorsam kapiyi emniyete almaliyim."
    lee_think "Iyi ki bu amaçla bir tutamaç yapmisim."

# game/scenes/chapter05/intro.rpy:60
translate turkish scene_chapter05_intro_5942017f:

    # lee_think "{e=storm}{e=dash}{e=bungalow4}{e=fearful_face}"
    lee_think "Umarim firtina tüm kulübeyi havaya uçurmaz."
    lee_think "Rüzgarin ugultusu o kadar yüksek ki, bundan sonra ne olabilecegine dair hiçbir fikrim yok."

# game/scenes/chapter05/intro.rpy:83
translate turkish scene_chapter05_intro_fb2312af:

    # lee "{e=surprised}{e=exclamation_white}{e=catgirl}"
    lee "Kim var orda? Cat?! Lanet olsun, az kalsin kafana tasi atiyodum.."

# game/scenes/chapter05/intro.rpy:84
translate turkish scene_chapter05_intro_b95f5b3e:

    # lee_think "{e=flushed_face}{e=catgirl}{e=question}{e=thinking_face}"
    lee_think "Buraya nasil girdi? Büyük ihtimal ben disardayken girmis olmali,"
    lee_think "onu uzun zamandir bu kadar sessiz görmemistim."

# game/scenes/chapter05/intro.rpy:85
translate turkish scene_chapter05_intro_d8e7b7c4:

    # lee_think "{e=thinking_face}{e=catgirl}{e=arrow_right}{e=scream}{e=storm}"
    lee_think "Belki de firtinadan korktugu için buraya geldi,"
    lee_think "ya da sadece bir yerlerde saklanmak istemis de olabilir."

