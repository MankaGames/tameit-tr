# game/scenes/chapter12/cave.rpy:104
translate turkish scene_chapter12_cave_secretroom_d85d0ba0:

    # lee_think "{e=wall}{e=torch}{e=torch}{e=question_white}{e=astonished_face}"
    lee_think "Bunlar mesale mi? Nereden gelmisler?"
    lee_think "Yakindan bakildiginda, duvarin sonradan yapimis oldugu anlasiliyor. Çok pürüzsüz."
    lee_think "Sonra burada da içinden balta basini buldugum gizli kutu vardi..."
    lee_think "Bunlar bir uygarligin izleri gibi görünüyor. Belki de adanin yerlileriydi."

# game/scenes/chapter12/cave.rpy:106
