# game/scenes/chapter06/lake_area.rpy:12
translate turkish scene_chapter06_rockway_1b8efc13:

    # lee_think "{e=astonished_face}{e=rock}{e=arrow_up}{e=exclamation_white}"
    lee_think "Burda bir sürü tas var! Bunu gelecekte lazim olur diye not edeyim."

# game/scenes/chapter06/lake_area.rpy:13
translate turkish scene_chapter06_rockway_6ca4acd1:

    # lee_think "{e=smiley}{e=hive}{e=exclamation_white}"
    lee_think "Oh, bir ari kovani! Bugün sansli günüm olmali."
    lee_think "Sanirim, buradan bal toplayabilirim."

# game/scenes/chapter06/lake_area.rpy:14
translate turkish scene_chapter06_rockway_733f38e0:

    # lee_think "{e=smoke}{e=no}{e=eyes}"
    lee_think "Artik dumani göremiyorum."
    lee_think "Acaba firtina o kamp atesini de benimkini yok ettigi gibi yok etti mi?"
    lee_think "Dumanin bu yönden geldigine yemin edebilirim. Yürümeye devam edeyim."

# game/scenes/chapter06/lake_area.rpy:33
translate turkish scene_chapter06_lake_shore_99c89fe2:

    # lee_think "{e=astonished_face}{e=lake}{e=smiley}"
    lee_think "Vaay! Burda bir göl varmis."

# game/scenes/chapter06/lake_area.rpy:34
translate turkish scene_chapter06_lake_shore_c2475901:

    # lee_think "{e=lake}{e=water}{e=exclamation_white}"
    lee_think "Aman tanrim, tüm göl tatli suyla dolu! Bunu kim düsünebilirdi."
    lee_think "Köprüyü insa ettikten sonra o firtina kütügü ikiye yarmasaydi,"
    lee_think "burayi hiç kesfetmemis olurdum. Demek ki her iste bir hayir var."

# game/scenes/chapter06/lake_area.rpy:35
translate turkish scene_chapter06_lake_shore_72011906:

    # lee_think "{e=swim}{e=fishing_pole}{e=smiley}"
    lee_think "Burada özgürce yüzebilir ve balik tutabilirim."

# game/scenes/chapter06/lake_area.rpy:36
translate turkish scene_chapter06_lake_shore_ddc7c836:

    # lee_think "{e=airplane}{e=astonished_face}{e=exclamation_white}"
    lee_think "Tanrim! Bu bir uçak! Göle o kadar dalmisim ki ilk basta farketmedim bile."
    lee_think "Yaklasip incelemeliyim."

# game/scenes/chapter06/lake_area.rpy:37
translate turkish scene_chapter06_lake_shore_28cac388:

    # lee_think "{e=thinking_face}{e=smoke}{e=arrow_right}{e=tonya}{e=question_white}"
    lee_think "Süphesiz, duman burdan bir yerlerden geliyor. Eger bu bizim ucaksa —"
    lee_think "Tonya da yasiyor olabilir demektir!"

# game/scenes/chapter06/lake_area.rpy:54
translate turkish scene_chapter06_wrecks_43e34169:

    # lee_think "{e=lee}{e=arrow_right}{e=airplane}{e=arrow_down}{e=desert island}"
    lee_think "Bu kesinlikle bizim düsen uçak."
    lee_think "Daha yakindan da bakiyim evet cidden bizim uçak."

# game/scenes/chapter06/lake_area.rpy:55
translate turkish scene_chapter06_wrecks_6baef6e3:

    # lee_think "{e=tonya}{e=parachute}{e=desert island}{e=thinking_face}{e=question_white}"
    lee_think "Eger Tonya da parasütle atlayabildiyse —"
    lee_think "o da bu adada demektir. Onu bulmaya çalismaliyim."
    lee_think "Tonya belki de bu tepenin arkasindadir."

# game/scenes/chapter06/lake_area.rpy:56
translate turkish scene_chapter06_wrecks_5f71f848:

    # lee "{e=tonya}{e=exclamation_white}{e=tonya}{e=exclamation_white}"
    lee "Tonya! Tonya, burada misin? Beni duyabiliyor musun? Benim, Lee!"

# game/scenes/chapter06/lake_area.rpy:57
translate turkish scene_chapter06_wrecks_6c1081f5:

    # lee_think "{e=ear}{e=no}{e=weary_face}{e=frowning_face}"
    lee_think "Tabii ki o burada degil. Çok aptalim! Ama en azindan denedim."

# game/scenes/chapter06/lake_area.rpy:79
translate turkish scene_chapter06_lake_9c275715:

    # lee_think "{e=fish}{e=arrow_up}"
    lee_think "Bahse girerim burada kolayca yakalayabilecegim çok çesitli baliklar vardir."
    lee_think "Evimin oradaki nehire kiyasla burda balik tutmak çocuk oyuncagi olacak."

# game/scenes/chapter06/lake_area.rpy:80
translate turkish scene_chapter06_lake_15c6553d:

    # lee_think "{e=catgirl}{e=smiling_face_with_hearts}{e=fish}"
    lee_think "Arti, Cat de bu baliklari çok sevecek!"

# game/scenes/chapter06/lake_area.rpy:86
translate turkish scene_chapter06_lake_1e44eb16:

    # lee_think "{e=light_bulb}{e=fish_big}{e=gift}{e=arrow_right}{e=catgirl}"
    lee_think "Büyük bir balik yakalabilirsem çok iyi olur. Cat kesinlikle buna bayilacak."
   

# game/scenes/chapter06/lake_area.rpy:62
translate turkish scene_chapter06_wrecks_02861276:

    # lee_think "{e=eyes}{e=magnifying_glass}{e=airplane}"
    lee_think "Su uçaga bi bakiyim. Belki içeri girmenin bir yolunu bulurum."

# game/scenes/chapter06/lake_area.rpy:63
translate turkish scene_chapter06_wrecks_f87c0b76:

    # lee_think "{e=thinking_face}{e=tonya}{e=magnifying_glass}{e=question_white}"
    lee_think "Kim bilir, belki Tonya'ya ait bir iz bulurum."

# game/scenes/chapter06/lake_area.rpy:70
translate turkish scene_chapter06_wrecks_climb_to_plane_59d0272e:

    # lee_think "{e=airplane}{e=arrow_up}{e=confused_face}"
    lee_think "Çok yüksek. Yukari çikmak zor olacak."

# game/scenes/chapter06/lake_area.rpy:71
translate turkish scene_chapter06_wrecks_climb_to_plane_60629747:

    # lee_think "{e=thinking_face}{e=arrow_right}{e=airplane}{e=question_white}"
    lee_think "Yine de içeri nasil girecegim? bunun bir yolunu bulmaliyim."

# game/scenes/chapter06/lake_area.rpy:72
translate turkish scene_chapter06_wrecks_climb_to_plane_e078dc3c:

    # lee_think "{e=light_bulb}{e=grappling}{e=exclamation_white}"
    lee_think "Bunu neden daha önce düsünemedim!"
    lee_think "Oraya çikmak için bir çesit kanca yapabilirim."

# game/scenes/chapter06/lake_area.rpy:73
translate turkish scene_chapter06_wrecks_climb_to_plane_9dce1d1e:

    # lee_think "{e=thinking_face}{e=hammer}{e=hook}{e=question_white}"
    lee_think "Sadece küçük bir sorun var - bir kancayi nerede bulabilirim?"
    lee_think "Bunu yapmak için ne kullanacagim?"

# game/scenes/chapter06/lake_area.rpy:74
translate turkish scene_chapter06_wrecks_climb_to_plane_81d4859a:

    # lee_think "{e=plane_door}{e=no}{e=arrow_up}"
    lee_think "Uçagin kapisi kapali."
    lee_think "Sadece oraya tirmanmam degil, ayni zamanda bir sekilde kapiyi da açmam gerekiyor."

# game/scenes/chapter06/lake_area.rpy:75
translate turkish scene_chapter06_wrecks_climb_to_plane_0ae9780b:

    # lee_think "{e=weary_face}{e=dont_know}{e=thinking_face}{e=hourglass}{e=arrow_left}"
    lee_think "Nasil yapacagim hakkinda hiçbir fikrim yok."
    lee_think "Iyice düsünüp daha sonra buraya gelmem gerekiyor."

