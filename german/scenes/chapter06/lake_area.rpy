# game/scenes/chapter06/lake_area.rpy:12
translate german scene_chapter06_rockway_1b8efc13:

    # lee_think "{e=astonished_face}{e=rock}{e=arrow_up}{e=exclamation_white}"
    lee_think "Hier gibt es so viele Steine!"
    lee_think "Das sollte ich mir unbedingt für die Zukunft merken."

# game/scenes/chapter06/lake_area.rpy:13
translate german scene_chapter06_rockway_6ca4acd1:

    # lee_think "{e=smiley}{e=hive}{e=exclamation_white}"
    lee_think "Oh, ein Bienenstock! Das muss mein Glückstag sein!"
    lee_think "Der, den ich bisher benutzte, wurde von den Flammen zerstört."
    lee_think "Einen Moment lang dachte ich, es gäbe keinen Honig mehr für mich."

# game/scenes/chapter06/lake_area.rpy:14
translate german scene_chapter06_rockway_733f38e0:

    # lee_think "{e=smoke}{e=no}{e=eyes}"
    lee_think "Ich kann den Rauch nicht mehr sehen."
    lee_think "Vielleicht hat der Sturm das Lagerfeuer zerstört, so wie meins."
    lee_think "Ich bin sicher, dass der Rauch in dieser Richtung lag. Ich muss weitermachen."

# game/scenes/chapter06/lake_area.rpy:33
translate german scene_chapter06_lake_shore_99c89fe2:

    # lee_think "{e=astonished_face}{e=lake}{e=smiley}"
    lee_think "Wahnsinn! Es hat sich herausgestellt, dass es hier einen See gibt!"

# game/scenes/chapter06/lake_area.rpy:34
translate german scene_chapter06_lake_shore_c2475901:

    # lee_think "{e=lake}{e=water}{e=exclamation_white}"
    lee_think "Meine Güte, ein ganzer See voller Süßwasser, wer hätte das gedacht!"
    lee_think "Wenn ich die Brücke nicht gebaut hätte und der Sturm nicht gekommen wäre"
    lee_think "Dann hätte ich diesen Ort nicht einmal entdeckt."

# game/scenes/chapter06/lake_area.rpy:35
translate german scene_chapter06_lake_shore_72011906:

    # lee_think "{e=swim}{e=fishing_pole}{e=smiley}"
    lee_think "Hier kann ich schwimmen und angeln."

# game/scenes/chapter06/lake_area.rpy:36
translate german scene_chapter06_lake_shore_ddc7c836:

    # lee_think "{e=airplane}{e=astonished_face}{e=exclamation_white}"
    lee_think "Oje! Da ist ein Flugzeug!"
    lee_think "Ich war so in den See vertieft, dass ich es zuerst gar nicht bemerkt habe."
    lee_think "Ich sollte mir das genauer ansehen."

# game/scenes/chapter06/lake_area.rpy:37
translate german scene_chapter06_lake_shore_28cac388:

    # lee_think "{e=thinking_face}{e=smoke}{e=arrow_right}{e=tonya}{e=question_white}"
    lee_think "Zweifellos kam der Rauch von irgendwo dort."
    lee_think "Wenn das das gleiche Flugzeug ist, besteht die Chance, dass Tonya auch überlebt hat!"

# game/scenes/chapter06/lake_area.rpy:54
translate german scene_chapter06_wrecks_43e34169:

    # lee_think "{e=lee}{e=arrow_right}{e=airplane}{e=arrow_down}{e=desert island}"
    lee_think "Das ist definitiv das Flugzeug, das mich hierher gebracht hat."
    lee_think "Wenn ich so davor stehe, es gibt keine zweifel"

# game/scenes/chapter06/lake_area.rpy:55
translate german scene_chapter06_wrecks_6baef6e3:

    # lee_think "{e=tonya}{e=parachute}{e=desert island}{e=thinking_face}{e=question_white}"
    lee_think "Wenn Tonya mit dem Fallschirm abgesprungen ist, wie ich es getan habe -"
    lee_think "könnte sie auch hier auf der Insel sein. Ich muss versuchen, sie zu finden!"
    lee_think "Vielleicht sollte ich nach ihr rufen?"
    lee_think "Was, wenn sie gar nicht so weit weg ist, direkt hinter diesem Hügel!"

# game/scenes/chapter06/lake_area.rpy:56
translate german scene_chapter06_wrecks_5f71f848:

    # lee "{e=tonya}{e=exclamation_white}{e=tonya}{e=exclamation_white}"
    lee "Tonja! Tonya, bist du hier?\nKannst du mich hören?\nIch bin's, Lee!"

# game/scenes/chapter06/lake_area.rpy:57
translate german scene_chapter06_wrecks_6c1081f5:

    # lee_think "{e=ear}{e=no}{e=weary_face}{e=frowning_face}"
    lee_think "Nun, natürlich ist sie nicht hier."
    lee_think "Wie dumm von mir! Aber ich habe es wenigstens versucht."

# game/scenes/chapter06/lake_area.rpy:79
translate german scene_chapter06_lake_9c275715:

    # lee_think "{e=fish}{e=arrow_up}"
    lee_think "Ich wette, hier gibt es viele verschiedene Fische"
    lee_think "die ich im Vergleich zu dem Wildfluss in meiner Gegend leicht fangen kann."

# game/scenes/chapter06/lake_area.rpy:80
translate german scene_chapter06_lake_15c6553d:

    # lee_think "{e=catgirl}{e=smiling_face_with_hearts}{e=fish}"
    lee_think "Außerdem würde Cat auch gerne leckeren Fisch essen!"

# game/scenes/chapter06/lake_area.rpy:86
translate german scene_chapter06_lake_1e44eb16:

    # lee_think "{e=light_bulb}{e=fish_big}{e=gift}{e=arrow_right}{e=catgirl}"
    lee_think "Ich habe kürzlich einen großen Fisch gefangen - Cat wird ihn sicher lieben."
    lee_think "Ich werde ihn ihr geben - sie liebt besondere Leckereien."

# game/scenes/chapter06/lake_area.rpy:62
translate german scene_chapter06_wrecks_02861276:

    # lee_think "{e=eyes}{e=magnifying_glass}{e=airplane}"
    lee_think "Ich sollte einen Blick auf das Flugzeug werfen. Vielleicht finde ich einen Weg hinein."

# game/scenes/chapter06/lake_area.rpy:63
translate german scene_chapter06_wrecks_f87c0b76:

    # lee_think "{e=thinking_face}{e=tonya}{e=magnifying_glass}{e=question_white}"
    lee_think "Wer weiß, vielleicht gibt es ein paar Spuren von Tonyas Anwesenheit."

# game/scenes/chapter06/lake_area.rpy:70
translate german scene_chapter06_wrecks_climb_to_plane_59d0272e:

    # lee_think "{e=airplane}{e=arrow_up}{e=confused_face}"
    lee_think "Zu hoch. Es wäre schwierig, dort hinaufzukommen."

# game/scenes/chapter06/lake_area.rpy:71
translate german scene_chapter06_wrecks_climb_to_plane_60629747:

    # lee_think "{e=thinking_face}{e=arrow_right}{e=airplane}{e=question_white}"
    lee_think "Aber wie komme ich hinein? Ich muss mir etwas einfallen lassen."

# game/scenes/chapter06/lake_area.rpy:72
translate german scene_chapter06_wrecks_climb_to_plane_e078dc3c:

    # lee_think "{e=light_bulb}{e=grappling}{e=exclamation_white}"
    lee_think "Warum ist mir das nicht früher eingefallen!"
    lee_think "Ich könnte eine Art Enterhaken basteln, um nach oben zu kommen."

# game/scenes/chapter06/lake_area.rpy:73
translate german scene_chapter06_wrecks_climb_to_plane_9dce1d1e:

    # lee_think "{e=thinking_face}{e=hammer}{e=hook}{e=question_white}"
    lee_think "Es gibt nur eine kleine Schwierigkeit - woher bekomme ich einen Haken?"
    lee_think "Woraus soll ich ihn herstellen?"

# game/scenes/chapter06/lake_area.rpy:74
translate german scene_chapter06_wrecks_climb_to_plane_81d4859a:

    # lee_think "{e=plane_door}{e=no}{e=arrow_up}"
    lee_think "Die Flugzeugtür ist ohnehin geschlossen."
    lee_think "Es geht darum, dass ich nicht nur hinaufklettern,"
    lee_think "sondern auch irgendwie die Tür öffnen muss."

# game/scenes/chapter06/lake_area.rpy:75
translate german scene_chapter06_wrecks_climb_to_plane_0ae9780b:

    # lee_think "{e=weary_face}{e=dont_know}{e=thinking_face}{e=hourglass}{e=arrow_left}"
    lee_think "Ich habe bis jetzt keine Ahnung, wie ich das machen soll."
    lee_think "ich muss mir etwas überlegen und später wiederkommen."

