    # lee_think "{e=no}{e=octa}{e=thinking_face}"
    lee_think "Es ist seltsam, dass Octa nicht hier ist."

# game/scenes/chapter12/cave.rpy:42
translate german scene_chapter12_cave1_1e302510:

    # lee "{e=eyes}{e=no}{e=crafting}{e=torch}"
    lee "Zu dunkel. Ohne Fackel kann ich nicht weitergehen."

# game/scenes/chapter12/cave.rpy:72
translate german scene_chapter12_cave2_4f981a47:

    # lee_think "{e=cave}{e=surprised}{e=exclamation_white}"
    lee_think "Was ist das? Eine Höhle? War sie schon immer hier?"
    lee_think "Sie ist mir nur noch nie aufgefallen oder…"

# game/scenes/chapter12/cave.rpy:73
translate german scene_chapter12_cave2_09133898:

    # lee_think "{e=catgirl}{e=octa}{e=lever}{e=rock}{e=smiley}"
    lee_think "Oh, es ist genau dieser Ort!"
    lee_think "Sieht so aus, als hätten Octa und Cat den Stein weggeräumt,"
    lee_think "der den Weg versperrt."

# game/scenes/chapter12/cave.rpy:104
translate german scene_chapter12_cave_secretroom_d85d0ba0:

    # lee_think "{e=wall}{e=torch}{e=torch}{e=question_white}{e=astonished_face}"
    lee_think "Sind das Fackeln?\nWoher kommen die?"
    lee_think "Bei näherem Hinschauen erkennt man, dass die Wand nicht echt ist. Sie ist so glatt."
    lee_think "Und dann war da noch diese steinerne Kiste mit einem geheimen Schloss,"
    lee_think "in der ich die Hellebarde gefunden habe…"
    lee_think "Das sind offenbar die Spuren einer Zivilisation. Vielleicht war die Insel besiedelt."

# game/scenes/chapter12/cave.rpy:106
translate german scene_chapter12_cave_secretroom_1a3049e4:

    # lee_think "{e=fire}{e=torch}{e=question_white}{e=thinking_face}"
    lee_think "Und wenn ich die Fackeln anzünde?"
    lee_think "Mit ihrem Licht kann ich den Ort leichter untersuchen."

# game/scenes/chapter12/cave.rpy:114
translate german scene_chapter12_cave_secretroom_8b5d7e19:

    # lee_think "{e=eyes}{e=wall}{e=framed_picture}{e=astonished_face}{e=exclamation_white}"
    lee_think "Hier ist etwas gemalt!\nDort in der Ecke."
    lee_think "Sieht aus wie ein Teil eines riesigen Gemäldes."

# game/scenes/chapter12/cave.rpy:115
translate german scene_chapter12_cave_secretroom_75a7cd6a:

    # lee_think "{e=stone_tablet}{e=no}{e=framed_picture}{e=thinking_face}"
    lee_think "Viele Teile des Bildes fehlen."

# game/scenes/chapter12/cave.rpy:116
translate german scene_chapter12_cave_secretroom_47e2c46f:

    # lee_think "{e=stone_tablet}{e=arrow_down}{e=question_white}"
    lee_think "Könnten sie beim Einsturz der Mauer herausgefallen sein?"

# game/scenes/chapter12/cave.rpy:117
translate german scene_chapter12_cave_secretroom_7971bfda:

    # lee_think "{e=magnifying_glass}{e=cave}"
    lee_think "Ich muss hier nach ihnen suchen."

# game/scenes/chapter12/cave.rpy:119
translate german scene_chapter12_cave_secretroom_be83be70:

    # lee_think "{e=eyes}{e=exclamation_white}{e=stone_tablet}"
    lee_think "Da haben wir's!\nDa ist eins der Teile!"

# game/scenes/chapter12/cave.rpy:121
translate german scene_chapter12_cave_secretroom_eedb88be:

    # lee_think "{e=stone_tablet}{e=yes}{e=inventory}{e=smiley}"
    lee_think "Ich habe bereits passende Teile bei mir"

# game/scenes/chapter12/cave.rpy:122
translate german scene_chapter12_cave_secretroom_ec6c931f:

    # lee_think "{e=stone_tablet}{e=arrow_right}{e=wall}"
    lee_think "In Ordnung, ich muss versuchen,"
    lee_think "ein Teil dieses Steinpuzzles in die Wand zu setzen."

# game/scenes/chapter12/cave.rpy:123
translate german scene_chapter12_cave_secretroom_bb1516a8:

    # lee_think "{e=stone_tablet}{e=framed_picture}{e=magnifying_glass}{e=question_white}"
    lee_think "Ich frage mich, wo all die anderen Teile sind?"
    lee_think "Ich schätze, sie sind über die ganze Insel verstreut."
    lee_think "Wenn ich sie alle sammle, finde ich vielleicht einen Schatz."
    lee_think "Auf jeden Fall wird es erst klar,"
    lee_think "wenn ich das Bild vollständig zusammengesetzt habe."

# game/scenes/chapter12/cave.rpy:169
translate german scene_chapter12_cave_secretroom_open_map_eacb8c1d:

    # lee "{e=fearful_face}{e=exclamation}"
    lee "Was zum Teufel ist hier los?!\nEs schüttelt wie verrückt!"
    lee "Habe ich etwas getan, was nicht getan werden sollte?"
    lee "Sag mir nicht, dass es mich lebendig begraben wird!"

# game/scenes/chapter12/cave.rpy:185
translate german scene_chapter12_cave_secretroom_open_map_d7383af4:

    # lee_think "{e=wall}{e=arrow_down}{e=exclamation}{e=exploding_head}"
    lee_think "Die Wand! Sie sinkt!\nZuerst dachte ich, ich hätte Halluzinationen!"

# game/scenes/chapter12/cave.rpy:240
translate german scene_chapter12_cave_exit_ec4056e1:

    # lee_think "{e=jaguar}{e=exclamation}{e=cold_sweat}"
    lee_think "Der Jaguar!\nEr ist immernoch da."
    lee_think "Aber natürlich! Wo sollte er sonst sein?"
    lee_think "Zu hoffen, dass er einfach verschwindet, war dumm."

# game/scenes/chapter12/cave.rpy:241
translate german scene_chapter12_cave_exit_cb31715b:

    # lee_think "{e=angry}{e=jaguar}{e=crossed_swords}{e=muscle}{e=exclamation_white}"
    lee_think "Oh, warte nur! Das zahle ich dir heim, du Bastard!"

