# game/scenes/chapter05/intro.rpy:25
translate german scene_chapter05_intro_43576a43:

    # lee_think "{e=dash}{e=lightning}{e=confounded face}{e=weary_face}"
    lee_think "Das Wetter ist so beschissen. \nDer Wind fühlt sich an wie eine Tonne Ziegelsteine!"

# game/scenes/chapter05/intro.rpy:26
translate german scene_chapter05_intro_905a5088:

    # lee_think "{e=lee}{e=arrow_right}{e=bungalow4}{e=frowning_face}"
    lee_think "Ich habe leider keine andere Wahl als zu Hause zu bleiben."
    lee_think "Gut, dass ich es geschafft habe, den Bungalow fertig zu stellen,"
    lee_think "bevor draußen alles aus dem Ruder gelaufen ist."

# game/scenes/chapter05/intro.rpy:35
translate german scene_chapter05_intro_09ae621c:

    # lee_think "{e=hushed_face}{e=rain}{e=no}{e=bonfire}"
    lee_think "Verdammt, das Feuer ist völlig erloschen."
    lee_think "Ich würde nicht sagen, dass ich überrascht bin - es schüttet wie aus Eimern."

# game/scenes/chapter05/intro.rpy:36
translate german scene_chapter05_intro_4da11327:

    # lee_think "{e=thinking_face}{e=light_bulb}{e=fireplace}{e=arrow_right}{e=bungalow4}"
    lee_think "Und was soll ich jetzt tun? \nIch kann weder kochen noch mich aufwärmen."
    lee_think "Ich muss drinnen irgendwie einen Kamin bauen."

# game/scenes/chapter05/intro.rpy:38
translate german scene_chapter05_intro_3aa89023:

    # lee "{e=fearful_face}{e=scream}{e=storm}{e=exclamation}"
    lee "Es ist nicht mehr nur Donner - es ist ein Orkan, oder ist es eher ein Sturm?"
    lee "Wen kümmert das schon? Es ist einfach alles im Arsch!"

# game/scenes/chapter05/intro.rpy:58
translate german scene_chapter05_intro_529c7150:

    # lee_think "{e=arrow_right}{e=plane_door}{e=lock}"
    lee_think "Ich sollte die Tür fester machen, wenn ich nicht will, dass sie wegfliegt."
    lee_think "Gut, dass ich sie vorher gestärkt habe."

# game/scenes/chapter05/intro.rpy:60
translate german scene_chapter05_intro_5942017f:

    # lee_think "{e=storm}{e=dash}{e=bungalow4}{e=fearful_face}"
    lee_think "Ich hoffe, dass die Hütte nicht in die Luft fliegt."
    lee_think "Der Wind heult so heftig,"
    lee_think "dass ich mir in die Hose scheißen werde,wenn etwas passiert."

# game/scenes/chapter05/intro.rpy:83
translate german scene_chapter05_intro_fb2312af:

    # lee "{e=surprised}{e=exclamation_white}{e=catgirl}"
    lee "Wer ist denn da? Cat?! Verdammt, ich habe fast einen Ziegelstein geschissen."

# game/scenes/chapter05/intro.rpy:84
translate german scene_chapter05_intro_b95f5b3e:

    # lee_think "{e=flushed_face}{e=catgirl}{e=question}{e=thinking_face}"
    lee_think "Wie ist sie hierher gekommen?"
    lee_think "Ich bin sicher, dass sie nicht da war, als ich rauskam,"
    lee_think "und ich habe sie schon eine ganze Weile nicht mehr gesehen."

# game/scenes/chapter05/intro.rpy:85
translate german scene_chapter05_intro_d8e7b7c4:

    # lee_think "{e=thinking_face}{e=catgirl}{e=arrow_right}{e=scream}{e=storm}"
    lee_think "Vielleicht kam sie hierher, weil sie Angst vor dem Sturm hatte."
    lee_think "und sich einfach irgendwo verstecken wollte."

