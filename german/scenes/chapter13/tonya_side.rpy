# game/scenes/chapter13/tonya_side.rpy:16
translate german scene_chapter13_tonya_side_7285d524:

    # lee "{e=tonya}{e=eyes}{e=exclamation_white}{e=airplane}{e=smiley}"
    lee "Und hier ist das Flugzeug! Ich sah dich zum ersten Mal vom Cockpit aus"
    lee "und mir wurde klar, dass ich auf jeden Fall auf diese Seite der Insel kommen musste."

# game/scenes/chapter13/tonya_side.rpy:24
translate german scene_chapter13_tonya_side_80a03145:

    # lee_think "{e=hut}{e=exclamation_white}"
    lee_think "Das ist die Hütte, die ich von der anderen Seite der Öffnung aus gesehen habe."

# game/scenes/chapter13/tonya_side.rpy:25
translate german scene_chapter13_tonya_side_6721135a:

    # lee_think "{e=footprints}{e=arrow_right}{e=hut}{e=smiley}"
    lee_think "Jetzt kann ich sie erreichen."

# game/scenes/chapter13/tonya_side.rpy:26
translate german scene_chapter13_tonya_side_a420b536:

    # lee_think "{e=tonya}{e=hut}{e=question_white}"
    lee_think "Könnte Tonya dort irgendwo sein?"

# game/scenes/chapter13/tonya_side.rpy:27
translate german scene_chapter13_tonya_side_29c4b9b8:

    # lee_think "{e=desert island}{e=tonya}{e=hourglass}{e=question_white}{e=thinking_face}"
    lee_think "Könnte sie die ganze Zeit auf der Insel gelebt haben?\nOh, ich hoffe es wirklich!"

# game/scenes/chapter13/tonya_side.rpy:39
translate german scene_chapter13_tonya_plane_3b7ce584:

    # lee "{e=airplane}{e=eyes}{e=arrow_right}{e=tonya}{e=hut}"
    pass

# game/scenes/chapter13/tonya_side.rpy:44
translate german scene_chapter13_tonya_plane_1525a53f:

    # lee_think "{e=hammer}{e=plane_ladder}{e=arrow_right}{e=airplane}{e=thinking_face}"
    lee_think "Hmm, wir könnten hier eine Leiter bauen, die als Brücke und Abkürzung dient."

# game/scenes/chapter13/tonya_side.rpy:45
translate german scene_chapter13_tonya_plane_9d9a5e5e:

    # lee_think "{e=footprints}{e=no}{e=arrow_right}{e=cave}{e=arrow_right}"
    pass

# game/scenes/chapter13/tonya_side.rpy:65
translate german scene_chapter13_tonya_forest_3afb1120:

    # lee_think "{e=tonya}{ellipsis}"
    lee_think "Tonya…"

# game/scenes/chapter13/tonya_side.rpy:66
translate german scene_chapter13_tonya_forest_78674d18:

    # lee_think "{e=astonished_face}{e=tonya}{e=smiley}{e=sparkles}{e=exclamation_white}"
    lee_think "Es ist wirklich Tonya…"

# game/scenes/chapter13/tonya_side.rpy:129
translate german scene_chapter13_tonya_home_c1c3e5de:

    # lee_think "{e=flushed_face}"
    lee_think "Unglaublich…"

# game/scenes/chapter13/tonya_side.rpy:130
translate german scene_chapter13_tonya_home_21eb1482:

    # lee_think "{e=sleeping_face}{e=thought_balloon}{e=question_white}"
    lee_think "Ich träume doch nicht, oder?"

# game/scenes/chapter13/tonya_side.rpy:131
translate german scene_chapter13_tonya_home_9c5a24af:

    # lee "{e=tonya}{e=exclamation_white}{e=exclamation_white}"
    lee "Tonya!"

# game/scenes/chapter13/tonya_side.rpy:135
translate german scene_chapter13_tonya_home_a843e1c6:

    # tonya "{e=lee}{e=question_white}{e=face_without_mouth}{ellipsis}"
    tonya "Lee?.."

# game/scenes/chapter13/tonya_side.rpy:140
translate german scene_chapter13_tonya_home_3c28b4e8:

    # tonya "{e=smiley}{e=lee}{e=exclamation_white}{e=question_white}"
    tonya "Lee! Aber wie?\nWie kommt es, dass du hier bist?"

# game/scenes/chapter13/tonya_side.rpy:141
translate german scene_chapter13_tonya_home_8aa7352e:

    # tonya "{e=you}{e=lee}{e=question_white}{e=dizzy_face}"
    tonya "Bist du es wirklich?\nIch kann meinen Augen nicht trauen!"
    tonya "Du hast dich so sehr verändert!"
    tonya "Ich habe dich kaum wiedererkannt!"
    tonya "Aber ich muss auch ganz anders aussehen!"

# game/scenes/chapter13/tonya_side.rpy:142
translate german scene_chapter13_tonya_home_95cef622:

    # tonya "{e=take}{e=lee}{e=no}{e=sleeping_face}{e=thought_balloon}"
    tonya "Lass mich dich berühren,\ndenn ich kann einfach nicht\u00A0glauben,"
    tonya "dass du echt bist und ich nicht nur träume."

# game/scenes/chapter13/tonya_side.rpy:151
translate german scene_chapter13_tonya_home_a220db4f:

    # lee "{e=me}{e=hourglass}{e=magnifying_glass}{e=you}{e=tear_smile}"
    lee "Tonya…\nIch kann es auch nicht glauben."
    lee "Ich hoffte, dich zu finden, aber jetzt, da ich es geschafft habe"
    lee "scheinen die Dinge nicht real zu sein."
    lee "Ich habe so lange nach dir gesucht."

# game/scenes/chapter13/tonya_side.rpy:152
translate german scene_chapter13_tonya_home_0fd3e394:

    # tonya "{e=magnifying_glass}{e=question_white}"
    tonya "Du suchtest nach mir?"

# game/scenes/chapter13/tonya_side.rpy:153
translate german scene_chapter13_tonya_home_9b449926:

    # lee "{e=me}{e=arrow_right}{e=airplane}{e=no}{e=tonya}"
    lee "Ja.\nIch habe das Flugzeug gefunden."
    lee "Es ist auf meiner Seite der Insel abgestürzt."
    lee "Du müsstest es vom Hügel aus gesehen haben."
    lee "Aber du warst natürlich nicht da"
    lee "und ich habe keine Hinweise\u00A0gefunden, wo du sein könntest."
    lee "Aber ich verlor nie die Hoffnung und suchte weiter."
    lee "Ich sah Rauch, konnte aber nicht hierher kommen."

# game/scenes/chapter13/tonya_side.rpy:154
translate german scene_chapter13_tonya_home_0d9951f6:

    # tonya "{e=airplane}{e=arrow_lower_right}{e=parachute}"
    tonya "Und ich bin im letzten Moment mit dem Fallschirm abgesprungen."

# game/scenes/chapter13/tonya_side.rpy:155
translate german scene_chapter13_tonya_home_58933736:

    # tonya "{e=airplane}{e=arrow_down}{e=desert island}{e=pensive_face}"
    tonya "Ich dachte, ich könnte das Flugzeug landen, aber es stürzte ab."
    tonya "Die Bäume haben es wohl abgefedert und vor der totalen Zerstörung bewahrt."
    tonya "Ich habe es auf der anderen Seite gesehen, aber der Jaguar war die ganze Zeit da."
    tonya "Ich konnte nicht einmal einen Blick auf das Wrack werfen."

# game/scenes/chapter13/tonya_side.rpy:156
translate german scene_chapter13_tonya_home_a112e14b:

    # tonya "{e=you}{e=parachute}{e=no}{e=arrow_down}{e=ocean}"
    tonya "Warte mal! Dein Fallschirm hat sich nicht geöffnet und du bist ins Meer gefallen!"

# game/scenes/chapter13/tonya_side.rpy:158
translate german scene_chapter13_tonya_home_08b233b6:

    # tonya "{e=you}{e=no}{e=skull_and_crossbones}{e=question_white}" nointeract
    tonya "Wie hast du überlebt?" nointeract

# game/scenes/chapter13/tonya_side.rpy:162
translate german scene_chapter13_tonya_home_0eb007ef:

    # lee "{e=boom}{e=ocean}{e=face_with_head-bandage}{e=injured_leg}{e=confounded face}"
    lee "Das Schwimmen fiel mir schwer, nachdem ich ziemlich hart aufs Wasser -"
    lee "aufgeschlagen war und mir das Bein verletzt hatte."

# game/scenes/chapter13/tonya_side.rpy:163
translate german scene_chapter13_tonya_home_b49a5b61:

    # lee "{e=swim}{e=arrow_right}{e=desert island}{e=night}{e=dizzy_face}"
    lee "Ich weiß nicht, wie ich in der Dunkelheit ans Ufer kam, bevor ich ohnmächtig wurde."
    lee "Als ich zu mir kam, war es bereits Morgen."

# game/scenes/chapter13/tonya_side.rpy:164
translate german scene_chapter13_tonya_home_1e79e8f9:

    # lee "{e=magnifying_glass}{e=weed}{e=health}{e=relieved}"
    lee "Ich hatte das Glück, etwas Heilgras zu finden, dann Wasser und Essen."
    lee "Glücklicherweise wuchsen Bananenstauden und Kokospalmen direkt am Ufer."

# game/scenes/chapter13/tonya_side.rpy:165
translate german scene_chapter13_tonya_home_0ac9d722:

    # lee "{e=hourglass}{e=arrows_counterclockwise}{e=ok_hand}{e=slightly_smiling_face}"
    lee "Nach und nach lebte ich mich auf der Insel ein und gewöhnte mich an sie."
    lee "Ich habe mir sogar ein kleines Haus gebaut."

# game/scenes/chapter13/tonya_side.rpy:166
translate german scene_chapter13_tonya_home_62971f8d:

    # tonya "{e=thumbs_up}{e=smiling_face}"
    tonya "Das hast du wirklich gut gemacht!"
    tonya "Ich hatte gehofft, dass du es schaffen würdest."

# game/scenes/chapter13/tonya_side.rpy:169
translate german scene_chapter13_tonya_home_5c096595:

    # lee "{e=eyes}{e=desert island}{e=swim}{e=beaming_face}{e=sparkles}"
    lee "Ich schwamm zu der Insel, die ich in der Ferne sehen konnte."
    lee "Das war gar nicht so schwierig."

# game/scenes/chapter13/tonya_side.rpy:170
translate german scene_chapter13_tonya_home_31e729c5:

    # lee "{e=hammer}{e=camp}{e=sparkles}{e=muscle}"
    lee "Dann lernte ich schnell, was was ist, baute eine Hütte wie du und begann zu überleben."
    lee "Ich würde sagen, es ist eine ziemlich paradiesische Insel!"

# game/scenes/chapter13/tonya_side.rpy:171
translate german scene_chapter13_tonya_home_e9e68ef1:

    # tonya "{e=hushed_face}{e=superhero}{e=thumbs_up}{e=unamused_face}"
    tonya "Wow!\nDu hast nicht wie ein Rambo ausgesehen."
    tonya "Ich bin überrascht, dass es für dich so einfach war.\nGut gemacht!"

# game/scenes/chapter13/tonya_side.rpy:172
translate german scene_chapter13_tonya_home_7cc0320b:

    # tonya "{e=lee}{e=tonya}{e=desert island}{e=exploding_head}{e=smiley}"
    tonya "Ich kann nicht glauben, dass wir die ganze Zeit auf der gleichen Insel waren!"

# game/scenes/chapter13/tonya_side.rpy:173
translate german scene_chapter13_tonya_home_da469511:

    # lee "{e=jaguar}{e=exclamation_white}{e=weary_face}{e=no}{e=footprints}"
    lee "Das lag an der Eröffnung und dem verdammten Jaguar!"
    lee "Das ist der Grund, warum ich so lange gebraucht habe, um hierher zu kommen!"

# game/scenes/chapter13/tonya_side.rpy:175
translate german scene_chapter13_tonya_home_f0d63cea:

    # tonya "{e=footprints}{e=arrow_right}{e=tonya}{e=desert island}{e=question_white}" nointeract
    tonya "Aber wie hast du es am Ende geschafft?" nointeract

# game/scenes/chapter13/tonya_side.rpy:179
translate german scene_chapter13_tonya_home_d8f5c108:

    # lee "{e=cave}{e=mountain}{e=arrow_right}{e=tonya}{e=desert island}"
    lee "Dort gibt es eine Höhle.\nDu musst sie selbst gesehen\u00A0haben."
    lee "Sie führt direkt durch den Berg und ich habe sie benutzt, um hierher zu kommen."

# game/scenes/chapter13/tonya_side.rpy:180
translate german scene_chapter13_tonya_home_8fee0474:

    # lee "{e=jaguar}{e=wound}{e=me}{e=persevering}{e=skull_and_crossbones}"
    lee "Der Jaguar hätte mich fast getötet,"
    lee "aber ich konnte ihn mit einer Waffe und einem Schild töten."

# game/scenes/chapter13/tonya_side.rpy:181
translate german scene_chapter13_tonya_home_9bd40234:

    # lee "{e=me}{e=cold_sweat}{e=crossed_swords}{e=jaguar}{e=skull}"
    lee "Es war verdammt gruselig, aber ich habe es irgendwie geschafft."
    lee "Vielleicht hatte ich wieder Glück."

# game/scenes/chapter13/tonya_side.rpy:182
translate german scene_chapter13_tonya_home_f5182c16:

    # tonya "{e=thumbs_up}{e=muscle}{e=smiling_face}"
    tonya "Ich schätze, es war nicht einfach? Es ist toll, dass du nicht aufgegeben hast!"

# game/scenes/chapter13/tonya_side.rpy:186
translate german scene_chapter13_tonya_home_bd0fd65a:

    # lee "{e=magnifying_glass}{e=cave}{e=dark}{e=eyes}{e=enter}"
    lee "Ich fand die Höhle und fand den Ausgang in völliger Dunkelheit,"
    lee "nur um auf den Jaguar zu treffen."

# game/scenes/chapter13/tonya_side.rpy:187
translate german scene_chapter13_tonya_home_7ad2a3c6:

    # lee "{e=halberd}{e=crossed_swords}{e=jaguar}{e=muscle}{e=sparkles}"
    lee "Ich hatte eine großartige Waffe und habe ihn im Handumdrehen erledigt."

# game/scenes/chapter13/tonya_side.rpy:188
translate german scene_chapter13_tonya_home_d148a1d1:

    # tonya "{e=you}{e=crossed_swords}{e=jaguar}{e=question_white}{e=surprised}"
    tonya "Du hast ihn so leicht erledigt?!\nErstaunlich!"

# game/scenes/chapter13/tonya_side.rpy:190
translate german scene_chapter13_tonya_home_b9527a7f:

    # tonya "{ellipsis}{e=cold_sweat}{e=pointing_up}{e=desert island}{e=question_white}" nointeract
    tonya "…Allein auf der Insel zu sein, muss unheimlich und einsam gewesen sein."
    tonya "Wie hast du das bewältigt?" nointeract

# game/scenes/chapter13/tonya_side.rpy:195
translate german scene_chapter13_tonya_home_519c7649:

    # lee "{e=no}{e=pointing_up}{e=desert island}{e=relieved}"
    lee "Ich war nicht allein."

# game/scenes/chapter13/tonya_side.rpy:196
translate german scene_chapter13_tonya_home_1628026e:

    # lee "{e=eye}{e=girl}{e=cat}{e=arrow_right}{e=catgirl}"
    lee "Ich weiß, dass sich meine Geschichte nach totalem Blödsinn anhört,"
    lee "aber zuerst habe ich ein Katzenmädchen getroffen."
    lee "Stell dir das vor - ein Mensch und eine Katze, alles in einem!"
    lee "Ich habe solche Dinge nur in Comics und Filmen gesehen."
    lee "Ich glaubte sogar, ich würde mir etwas einbilden, weil ich zu lange in der Sonne war."
    lee "Irgendwann haben wir uns angefreundet, oder habe ich sie gezähmt -"
    lee "das kommt darauf an, wie man es sieht."

# game/scenes/chapter13/tonya_side.rpy:197
translate german scene_chapter13_tonya_home_f357b188:

    # lee "{e=lake}{e=girl}{e=octopus}{e=arrow_right}{e=octa}"
    lee "Und dann habe ich im See ein Oktopus-Mädchen gefunden."
    lee "Ich dachte, dort sei ein Monster, aber es war ein niedliches Wesen."
    lee "Sie ist nicht gesprächig, aber wissbegierig und freundlich."

# game/scenes/chapter13/tonya_side.rpy:198
translate german scene_chapter13_tonya_home_4f77c0de:

    # tonya "{e=astonished_face}{e=question_white}{e=exploding_head}{e=exclamation_white}"
    tonya "Was?! Das ist unglaublich! Wie ist das überhaupt möglich?!"
    tonya "Ich habe nichts in dieser Richtung gesehen. Nur den Jaguar, Möwen und Krabben."

# game/scenes/chapter13/tonya_side.rpy:199
translate german scene_chapter13_tonya_home_bf8b40d4:

    # tonya "{e=catgirl}{e=octa}{e=skull}{e=you}{e=question_white}"
    tonya "Haben sie nicht versucht, dich zu töten? Schließlich sind sie Monster."

# game/scenes/chapter13/tonya_side.rpy:200
translate german scene_chapter13_tonya_home_7f230d96:

    # lee "{e=hut}{e=catgirl}{e=octa}{e=lee}{e=happy_sweat}"
    lee "Nein… sie leben jetzt mit mir zusammen."

# game/scenes/chapter13/tonya_side.rpy:201
translate german scene_chapter13_tonya_home_d3ba1dfc:

    # lee "{e=catgirl}{e=octa}{e=handshake}{e=lee}{e=blush}"
    lee "Wir sind sozusagen… Freunde…"

# game/scenes/chapter13/tonya_side.rpy:202
translate german scene_chapter13_tonya_home_9b07d8c9:

    # tonya "{e=surprised}{e=exclamation_white}"
    tonya "Das ist absolut unglaublich."

# game/scenes/chapter13/tonya_side.rpy:203
translate german scene_chapter13_tonya_home_dacd6fbe:

    # tonya "{e=me}{e=handshake}{e=catgirl}{e=octa}{e=question_white}"
    tonya "Stellst du mich später vor?\nIch würde sie gerne sehen."

# game/scenes/chapter13/tonya_side.rpy:204
translate german scene_chapter13_tonya_home_fc14aa5b:

    # lee "{e=smiling_face}{e=yes}{e=exclamation_white}"
    lee "Na Klar!"

# game/scenes/chapter13/tonya_side.rpy:209
translate german scene_chapter13_tonya_home_c84f2d1d:

    # lee "{e=muscle}{e=steam_from_nose}{e=ok_hand}{e=beaming_face}"
    lee "Oh, das ist schon in Ordnung! Ich bin ein ehemaliger Pfadfinder!"
    lee "Ich habe mich schnell an meine Ausbildung erinnert und es war in Ordnung!"
    lee "Ich hatte keine Zeit, einsam zu sein oder Angst zu haben -"
    lee "ich musste mich aufs Überleben konzentrieren!"

# game/scenes/chapter13/tonya_side.rpy:210
translate german scene_chapter13_tonya_home_56e2a316:

    # tonya "{e=rolling_eyes}{e=unamused_face}"
    tonya "Oh, wirklich… und ich hatte eine harte Zeit hier."
    tonya "Es ist schade, dass wir uns nicht früher begegnet sind,"
    tonya "ich hätte deine Hilfe wirklich gebrauchen können."

# game/scenes/chapter13/tonya_side.rpy:212
translate german scene_chapter13_tonya_home_aecbb07a:

    # tonya "{e=pensive_face}{e=pointing_up}{e=desert island}{e=crying_face}"
    tonya "Ich war die ganze Zeit allein."
    tonya "Ich habe sogar ein Stockmännchen gemacht, das mir Gesellschaft leistet."

# game/scenes/chapter13/tonya_side.rpy:213
translate german scene_chapter13_tonya_home_6e705a56:

    # tonya "{e=skull}{e=desert island}{e=cold_sweat}{e=frowning_face}"
    tonya "Die ersten paar Tage waren am beängstigendsten."
    tonya "Ich dachte, ich würde hier sterben."

# game/scenes/chapter13/tonya_side.rpy:215
translate german scene_chapter13_tonya_home_51993bde:

    # tonya "{e=storm}{e=tired_face}" nointeract
    tonya "Und dann war da auch noch dieser schreckliche Sturm!" nointeract

# game/scenes/chapter13/tonya_side.rpy:219
translate german scene_chapter13_tonya_home_6072e746:

    # lee "{e=exclamation_white}{e=storm}{e=persevering}"
    lee "Absolut richtig! Dieser verdammte Sturm hat mein halbes Haus weggeblasen!"
    lee "Ich dachte, das wäre mein Tod."

# game/scenes/chapter13/tonya_side.rpy:220
translate german scene_chapter13_tonya_home_f7d6a78a:

    # tonya "{e=scream}{e=storm}{e=dash}{e=tent}"
    tonya "Das dachte ich auch!\nDas hat mich zu Tode erschreckt."
    tonya "Meine Hütte wurde komplett weggeblasen."

# game/scenes/chapter13/tonya_side.rpy:221
translate german scene_chapter13_tonya_home_3a06d57b:

    # tonya "{e=no}{e=bonfire}{e=cold_face}"
    tonya "Es war so kalt und ich konnte kein Feuer machen."

# game/scenes/chapter13/tonya_side.rpy:222
translate german scene_chapter13_tonya_home_7b44ca00:

    # lee "{e=pleading_face}{e=pensive_face}{e=me}{e=no}{e=hug}"
    lee "Es tut mir so leid, dass ich nicht da war! Gemeinsam wäre es einfacher gewesen."
    lee "Ich hätte dich beschützt und dir geholfen."

# game/scenes/chapter13/tonya_side.rpy:226
translate german scene_chapter13_tonya_home_2207b123:

    # lee "{e=rain}{e=ok_hand}{e=beaming_face}{e=bungalow4}{e=muscle}"
    lee "Nur ein stärkerer Regen.\nKeine große Sache."
    lee "Und meine Hütte erwies sich als recht stabil, also wurde sie nicht schwer beschädigt."

# game/scenes/chapter13/tonya_side.rpy:227
translate german scene_chapter13_tonya_home_d586ac17:

    # tonya "{e=rolling_eyes}{e=thumbs_up}"
    tonya "Ich freue mich, dass es für dich so gut gelaufen ist."

# game/scenes/chapter13/tonya_side.rpy:228
translate german scene_chapter13_tonya_home_88203352:

    # tonya "{e=hourglass}{e=night}{e=footprints}{e=sleeping_face}"
    tonya "Es ist bereits dunkel.\nLass uns ins Bett gehen."

# game/scenes/chapter13/tonya_side.rpy:234
translate german scene_chapter13_tonya_home_old_friend_67dd13e7:

    # lee "{e=relieved}{e=tonya}{e=handshake}{e=heart}{e=lee}"
    lee "Es ist gut, dass wir den Sturm überlebt haben und wieder zusammengekommen sind."

# game/scenes/chapter13/tonya_side.rpy:235
translate german scene_chapter13_tonya_home_old_friend_ad811db4:

    # tonya "{e=yes}{e=smiling_face}"
    tonya "Ja…"

# game/scenes/chapter13/tonya_side.rpy:236
translate german scene_chapter13_tonya_home_old_friend_d56e0ce3:

    # lee "{e=tonya}{ellipsis}{e=pleading_face}"
    lee "Tonya, weißt du…"

# game/scenes/chapter13/tonya_side.rpy:237
translate german scene_chapter13_tonya_home_old_friend_3eda9ade:

    # lee "{e=sleeping_face}{e=thought_balloon}{e=tonya}{e=smiling_face_with_hearts}"
    lee "…ich habe dich vermisst. Habe dich sogar in meinen Träumen gesehen."

# game/scenes/chapter13/tonya_side.rpy:239
translate german scene_chapter13_tonya_home_old_friend_f1c7896a:

    # tonya "{e=me}{e=blush}"
    tonya "Ich habe auch oft an dich gedacht."

# game/scenes/chapter13/tonya_side.rpy:240
translate german scene_chapter13_tonya_home_old_friend_477952aa:

    # lee "{ellipsis}"
    lee "..."

# game/scenes/chapter13/tonya_side.rpy:241
translate german scene_chapter13_tonya_home_old_friend_f7706428:

    # tonya "{e=kiss}{e=me}{e=smiling_face}{e=exclamation_white}"
    tonya "Küss mich schon, Dummerchen!"
    tonya "Wie kommt es, dass du noch nicht auf mich gesprungen bist?"

# game/scenes/chapter13/tonya_side.rpy:242
translate german scene_chapter13_tonya_home_old_friend_1f5d95ad:

    # lee "{e=star_struck}{e=heart_eyes}"
    lee "Es macht dir wirklich nichts aus? Ich habe so lange davon geträumt…"

# game/scenes/chapter13/tonya_side.rpy:243
translate german scene_chapter13_tonya_home_old_friend_0f3e957e:

    # tonya "{e=consume}{e=you}{e=beaming_face}{e=smirk}"
    tonya "Ich bin kurz davor, dich zu vernaschen!"

