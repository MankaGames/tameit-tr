# game/scenes/chapter09/tame.rpy:64
translate german scene_chapter09_tame_47690411:

    # lee_think "{e=gift}{e=smiling_face_with_hearts}{e=octa}{e=question_white}{e=thinking_face}"
    lee_think "Ich frage mich, was ihr gefallen würde."
    lee_think "Ich weiß, dass sie Obst mag, aber\u00A0könnte es etwas geben, das sie noch mehr mag?"

# game/scenes/chapter09/tame.rpy:65
translate german scene_chapter09_tame_d9093360:

    # lee_think "{e=t-shirt}{e=surprised}{e=exclamation_white}"
    lee_think "Oh, ist das nicht mein T-Shirt?\nWie ist es hierher gekommen?"
    lee_think "Und andere Sachen auch!"
    lee_think "Ich habe all diese Dinge nie bemerkt."
    lee_think "Ich schätze, Octa sammelt alles ein,"
    lee_think "was sie für interessant hält und bringt es hierher."

# game/scenes/chapter09/tame.rpy:75
translate german scene_chapter09_tame_59b58972:

    # lee_think "{e=octa}{e=comb}{e=hair}{e=take}"
    lee_think "Octa bürstete ihr Haar mit den Fingern, hmm…"

# game/scenes/chapter09/tame.rpy:76
translate german scene_chapter09_tame_6f61da61:

    # lee_think "{e=thinking_face}{e=gift}{e=comb}{e=arrow_right}{e=octa}"
    lee_think "Es wäre toll, ihr einen Kamm zu geben."
    lee_think "Das wird ein ungewöhnliches und\u00A0nützliches Geschenk für sie sein."

# game/scenes/chapter09/tame.rpy:78
translate german scene_chapter09_tame_01c66ba3:

    # lee_think "{e=magnifying_glass}{e=comb}{e=arrow_right}{e=airplane}{e=question_white}"
    lee_think "Aber wo finde ich einen Kamm?"
    lee_think "Soll ich ihn selbst machen? Ich\u00A0bezweifle, dass er gut wird."
    lee_think "Ich werde versuchen, das\u00A0Flugzeug noch einmal zu durchsuchen,"
    lee_think "um zu sehen, ob\u00A0ich etwas in dieser Art finde."

# game/scenes/chapter09/tame.rpy:105
translate german scene_chapter09_tame_done_b1819cac:

    # lee_think "{e=octa}{e=smiling_face_with_hearts}{e=gift}{e=smiley}{e=sparkles}"
    lee_think "Super Octa mag das\u00A0geschenk das ich ihr gegeben habe sehr!"

# game/scenes/chapter09/tame.rpy:106
translate german scene_chapter09_tame_done_2f84d4db:

    # lee_think "{e=lee}{e=handshake}{e=octa}{e=smiling_face}{e=exclamation_white}"
    lee_think "Meine Bemühungen haben sich gelohnt, und\u00A0es sieht so aus,"
    lee_think "als ob die kleinen Geschenke, die\u00A0ich für sie hinterlassen habe,"
    lee_think "uns ein wenig näher gebracht haben."

